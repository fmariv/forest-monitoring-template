{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": ["./src/lib"],
			"$lib/*": ["./src/lib/*"],
			"$stores": ["src/stores"],
			"$stores/*": ["src/stores/*"],
			"$components": ["src/components"],
			"$components/*": ["src/components/*"],
			"$styles": ["src/styles"],
			"$styles/*": ["src/styles/*"]
		},
		"ignoreDeprecations": "5.0"
	}
}
